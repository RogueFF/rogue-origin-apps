    "test:chromium": "playwright test kanban-tutorial.spec.js --project=chromium",
    "test:firefox": "playwright test kanban-tutorial.spec.js --project=firefox",
    "test:webkit": "playwright test kanban-tutorial.spec.js --project=webkit",
